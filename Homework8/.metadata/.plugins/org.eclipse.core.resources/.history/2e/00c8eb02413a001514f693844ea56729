package Account;

public class PersonalAccount extends Account {
protected String name;
protected long ID;

public PersonalAccount(String name, long ID) throws Exception{
	super(name, ID);
	this.name = name;
	this.ID = ID;
	setName(name);
	setID(ID);
}

public String getName() {
	return name;
}

public void setName(String name) {
	this.name = name;
}

public long getID() {
	return ID;
}

public void setID(long iD) {
	ID = iD;
}

	 
	
}
	

}
