
public class Zadacha4 {

	public static void main(String[] args) {
		int [][]matrix = new int[5][6];
		int n = 30;
		for (int i = 0; i < matrix.length; i++){
			for(int j = 0; j < matrix[i].length; j++){
				matrix[i][j] = n;
				n--;
				System.out.println(matrix[i][j] + "\t");
			}
			System.out.println();
		}

	}

}
