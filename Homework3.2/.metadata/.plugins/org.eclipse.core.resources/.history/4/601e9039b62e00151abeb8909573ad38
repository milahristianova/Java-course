import java.util.Scanner;
public class Zadacha4 {

	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		int N = input.nextInt();
		int sum = 0;
		for (int i = 1; i <= N; i++){
		if(N % 7 == 0){
		if (N % 11 == 0){
			continue;
		}
		sum += N;
		}
		System.out.println("Sum = " +sum);
		}

	}

}
