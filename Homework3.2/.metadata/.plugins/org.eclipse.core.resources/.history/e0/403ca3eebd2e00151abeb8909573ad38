import java.util.Scanner;
public class Zadacha5 {

	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		System.out.print("n = ");
		int n = input.nextInt();
		int k = input.nextInt();
		long factorial = 1;
   for (int i = 1; i <= n; i++) {
	   factorial *= i;
	for (int j = 1; j <= k; j++){
		factorial *=j;
		System.out.println(n +"! = " + factorial % k + "! = " + factorial);
	}
   }

	}

}
