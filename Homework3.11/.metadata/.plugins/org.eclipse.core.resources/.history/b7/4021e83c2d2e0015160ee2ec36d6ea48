import java.util.Scanner;
public class Zadacha2 {

	public static void main(String[] args) {
Scanner scanner = new Scanner(System.in);
int x = scanner.nextInt();
int y = scanner.nextInt();		
		
		if (x < 0 ){
		if (y < 0 ){
			System.out.println("-");
		}else{  if (x > 0 );
		  if (y > 0 );}    
			System.out.println("+");
		}
		}
        
	}


