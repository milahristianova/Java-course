import java.util.Scanner;
public class Zadacha1 {

	public static void main(String[] args) {
		Scanner scanner = new Scanner(System.in);
		
		int number = 6;
		switch(number)
		{
		case 1 :
			System.out.println("The number is odd");
			break;
		case 2 :
			System.out.println("The number is even");
			break;
		case 3 :
			System.out.println("The number is odd");
			break;
		case 4 :
			System.out.println("The number is even");
			break;
		case 5 :
			System.out.println("The number is odd");
			break;
		}
		
		
		
	

	}

}
