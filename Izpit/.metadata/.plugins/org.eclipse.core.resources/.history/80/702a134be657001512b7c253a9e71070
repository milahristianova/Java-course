import java.util.Scanner;

public class Test {

	private static int[][] arrays;

	public static void main(String[] args) {
		arrays = null;
		{
			

			for (int i = 0; i < arrays.length; i++) {
		        int sum = 0;
		        int j = 0;
				
				sum = sum + arrays[i][j];
				System.out.println("The sum is: " + sum);
			}

	}

	}
}

