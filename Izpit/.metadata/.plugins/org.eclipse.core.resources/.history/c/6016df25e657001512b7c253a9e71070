import java.util.Scanner;

public class Test {

	public static void main(String[] args) {
		int[][] array = null;
		{
			

			for (int i = 0; i < array.length; i++) {
		        int sum = 0;
		        int j = 0;
				
				sum = sum + array[i][j];
				System.out.println("The sum is: " + sum);
			}

	}

	}
}

